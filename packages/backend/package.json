{
  "name": "@crypto-pos/backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run generate:prisma && tsc",
    "generate:prisma": "prisma generate",
    "generate:graphql": "graphql-codegen",
    "start": "node --experimental-specifier-resolution=node  --loader ts-node/esm ./dist/index.js",
    "dev": "concurrently \"tsc --watch\" \"npm start\"",
    "db:seed": "prisma db seed",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "keywords": [
    "graphql",
    "typescript",
    "prisma",
    "apollo-server",
    "express"
  ],
  "author": "George Phillips <georgephillips22@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-tools/schema": "^10.0.4",
    "@noble/hashes": "^1.4.0",
    "@pothos/core": "^4.0.2",
    "@pothos/plugin-prisma": "^4.0.3",
    "@prisma/client": "^5.16.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "morgan": "^1.10.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "prisma": "^5.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
